@model MilkyProject.DtoLayer.CategoryDtos.CreateCategoryDto
@{
    ViewData["Title"] = "CreateCategory";
}

<div class="container">
    <h1>Kategori Oluştur</h1>
    <hr />
    <div class="row">
        <div class="col-12">
            <form method="post">
                <label>Kategori Adı</label>
                <input type="text" asp-for="Name" class="form-control" />
                <button class="btn btn-primary">Ekle</button>
            </form>
        </div>
    </div>
</div>
