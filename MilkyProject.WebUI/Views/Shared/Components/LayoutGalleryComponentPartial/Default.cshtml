@model List<ResultGalleryDto>

<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-3">
                <a href="@item.ImageUrl" data-lightbox="gallery">
                    <img class="img-fluid" src="@item.ImageUrl" alt="" style="width:100%; height:100%; object-fit:cover;">
                </a>
            </div>
        }
    </div>
</div>

           
